    "agent",
    "trik",
    "gateway",
    "security",
    "prompt-injection",
    "langchain",
    "typescript",
    "trikhub"
  ],
  "author": "Muffles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Muffles/trikhub.git",
    "directory": "packages/trik-gateway"
  },
  "bugs": {
    "url": "https://github.com/Muffles/trikhub/issues"
  },
  "homepage": "https://github.com/Muffles/trikhub/tree/main/packages/trik-gateway#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
